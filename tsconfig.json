{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/dashboard/component/ProjectManager/CalendarProject.jsx",
    "app/dashboard/component/ProjectManager/ScheduledInterviewsGraph.tsx",
    "app/login/page.js"
, "app/dashboardx/P_M_Todo0/page.js", "app/login/page.js", "app/page.js", "store/reducers/meetingReducer.js", "store/reducers/dashboard.js", "store/reducers/calenderReducer.js"  ],
  "exclude": [
    "node_modules"
  ]
}
